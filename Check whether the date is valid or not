year = int(input("year: "))
m = int (input("month: "))
d = int(input("day: "))
leap = (year %4 ==0 and year % 100 !=0)or(year %400 == 0)
mon=[31,28+leap,31,30,31,30,31,31,30,31,30,31]
if m<1 or m>12 or d<1 or d>mon[m-1]:
	print("invalid")
else:
	print("valid")
	d+=1
	if d>=mon[m-1]:
		d=1
		m+=1
		if m>12:
			m=1
			year+=1
	print(f"incremented date: {year}-{m:02}-{d:02d}")
